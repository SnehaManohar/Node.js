<html>
  <head>
    <title>RESULT.EJS</title>
  </head>
  <body>
    <h4>Students details</h4>
    <table border="1px">
      <tr>
        <th>
          USN
        </th>
        <th>
          NAME
        </th>
        <th>
          BRANCH
        </th>
        <th>
          SEM
        </th>
      </tr>
      <% for (var i = 0; i < data.length; i++) { %>
      <tr>
        <td>
          <%= data[i].usn %>
        </td>
        <td>
          <b><%= data[i].name %><b>
        </td>
        <td>
          <u><%= data[i].branch %><u>
        </td>
        <td>
          <%= data[i].sem %>
        </td>
      </tr>
      <% } %>
    </table>

    <br />
    <%= i %>
    <a href="/">Home</a><br />
    <a href="/insert">Add new</a><br />
    <a href="/find">Search</a>
  </body>
</html>