<html>
  <head>
    <title>RESULT.EJS</title>
  </head>
  <body>
    <h4>Students details</h4>

    <table border="1px">
      <tr>
        <th>
          USN
        </th>
        <th>
          NAME
        </th>
        <th>
          COMPANY
        </th>
      </tr>
      <% for (var i = 0; i < data.length; i++) { %>
      <tr>
        <td>
          <%= data[i].usn %>
        </td>
        <td>
          <%= data[i].name %>
        </td>
        <td>
          <%= data[i].company %>
        </td>        
      </tr>
      <% } %>
    </table>
    <p>Number of students <%=i %></p>
    <br />

    <a href="/">Home</a><br />
    <a href="/insert">Add new</a><br />
    <a href="/find">Search</a>
  </body>
</html>